@model WebApplication1.Models.DashboardViewModel

<h2>My Dashboard</h2>

<h3>Upcoming Events</h3>
<ul>
    @foreach(var ev in Model.UpcomingTickets)
    {
        <li>@ev.Event.Title - @ev.Event.EventDate.ToLocalTime().ToString("g")</li>
    }
</ul>

<h3>My Tickets</h3>
<ul>
    @foreach(var ticket in Model.UpcomingTickets)
    {
        <li>@ticket.EventName - @ticket.EventDate.ToLocalTime().ToString("g")</li>
    }
</ul>

<h3>Purchase History</h3>
<ul>
    @foreach(var ticket in Model.PurchaseHistory)
    {
        <li>@ticket.EventName - @ticket.EventDate.ToLocalTime().ToString("g")</li>
    }
</ul>